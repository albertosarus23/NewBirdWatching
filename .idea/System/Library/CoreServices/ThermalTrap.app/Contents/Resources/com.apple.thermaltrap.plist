<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.apple.thermaltrap</string>
	<key>ProgramArguments</key>
	<array>
		<string></string>
	</array>
	<key>LaunchEvents</key>
	<dict>
		<key>com.apple.notifyd.matching</key>
		<dict>
			<key>com.apple.system.power.thermal_warning</key>
			<dict>
				<key>Notification</key>
				<string>com.apple.system.power.thermal_warning</string>
			</dict>
			<key>com.apple.system.power.performance_warning</key>
			<dict>
				<key>Notification</key>
				<string>com.apple.system.power.performance_warning</string>
			</dict>
		</dict>
	</dict>
	<key>Program</key>
	<string>/System/Library/CoreServices/ThermalTrap.app/Contents/MacOS/ThermalTrap</string>
	<key>ProcessType</key>
	<string>App</string>
	<key>KeepAlive</key>
	<dict>
		<key>PathState</key>
		<dict>
			<key>/private/var/run/thermal/.thermalpressure</key>
			<true/>
		</dict>
	</dict>
</dict>
</plist>
